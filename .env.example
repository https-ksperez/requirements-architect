# LlamaCloud API Configuration
LLAMA_CLOUD_API_KEY=your-api-key-here
LLAMA_CLOUD_PROJECT_ID=your-project-id-here

# Environment Configuration
LLAMA_CLOUD_ENV=prod  # or 'staging'

# Windows UTF-8 Configuration
# Required to fix UnicodeDecodeError on Windows systems
# Set these environment variables to force UTF-8 encoding
PYTHONIOENCODING=utf-8
PYTHONUTF8=1
